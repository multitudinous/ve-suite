#!/bin/bash

source __VE_SUITE_ENV_FILE_PLACEHOLDER__

if [ -z $VES_LOG_LEVEL ]
then
    export VES_LOG_LEVEL=warning
fi

ves_xplorer --jconf=${VES_PREFIX}/share/vesuite/vrj_configs/stereo_desktop/desktop.jconf --VESRTT --VESDesktopMode --VESLog=$VES_LOG_LEVEL --VESLogPath="${VES_PREFIX}"/debug.log
