
Name: Bullet
Version: 2.77
Build Arguments:
  ccmake
    BUILD_CPU_DEMOS to OFF
    BUILD_DEMOS to OFF
    BUILD_EXTRAS to ON
    BUILD_MINICL_OPENCL_DEMOS to OFF
    BUILD_UNIT_TESTS to OFF
    CMAKE_BUILD_TYPE to RelWithDebInfo
    CMAKE_INSTALL_PREFIX to ${BULLET_INSTALL_DIR}
    INCLUDE_INSTALL_DIR to ${BULLET_INSTALL_DIR}/include
    INSTALL_EXTRA_LIBS to ON
    INSTALL_LIBS to ON
    LIB_DESTINATION to ${BULLET_INSTALL_DIR}/lib
    PKGCONFIG_INSTALL_PREFIX to ${BULLET_INSTALL_DIR}/lib/pkgconfig
    USE_CUSTOM_VECTOR_MATH to OFF
    USE_DOUBLE_PRECISION to OFF
    USE_GLUT to OFF
    USE_GRAPHICAL_BENCHMARK to OFF
    USE_MSVC_RUNTIME_LIBRARY_DLL to OFF
  make install
OS: Mac OS X 10.6
Architecture: x86_64

