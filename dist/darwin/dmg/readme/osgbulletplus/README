
Name: osgBulletPlus
Version: 1.1.50
Build Arguments:
  ccmake
    CMAKE_BUILD_TYPE to RelWithDebInfo
    CMAKE_INSTALL_PREFIX to ${OSGBULLETPLUS_INSTALL_DIR}
    OSGBULLETPLUS_BUILD_APPLICATIONS to ON
    OSGBULLETPLUS_BUILD_EXAMPLES to OFF
    OSGBULLETPLUS_BUILD_PROTOS to OFF
    OSGBULLETPLUS_BUILD_TESTS to OFF
    OSGBULLETPLUS_USE_DOUBLE_PRECISION to OFF
    OSGBULLET_ROOT to ${BULLET_INSTALL_DIR}
    BulletInstallType to Alternate Install Location
    OSGInstallType to Alternate Install Location
    BulletInstallLocation to ${BULLET_INSTALL_DIR}
    OSGInstallLocation to ${OSG_INSTALL_DIR}
  make install
OS: Mac OS X 10.6
Architecture: x86_64

