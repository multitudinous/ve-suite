         <plugin_path>${VJ_BASE_DIR}/lib/gadgeteer-2.1.29/plugins/</plugin_path>
         <plugin>RIMPlugin</plugin>
         <plugin>ApplicationDataManager</plugin>
	<!--
         <cluster_node>Node(viz1)</cluster_node>
